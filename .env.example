# Site URL (required for Mini App)
NEXT_PUBLIC_SITE_URL=https://your-domain.vercel.app

# OnchainKit API Key (get from https://portal.cdp.coinbase.com/)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=

# OpenRouter API Key for FUD/Good News generation (get from https://openrouter.ai/keys)
OPENROUTER_API_KEY=

# dRPC API Key for Base RPC (get from https://drpc.org/)
DRPC_API_KEY=

# Vercel Blob Token for LLM cache (get from https://vercel.com/docs/storage/vercel-blob)
BLOB_READ_WRITE_TOKEN=
